<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none" default-lazy="false" package="com.rsmart.certification.impl.hibernate.criteria.gradebook">
    <class name="com.rsmart.certification.impl.hibernate.criteria.gradebook.CertGradingEvent" table="GB_GRADING_EVENT_T" discriminator-value="0">
        <id name="id" column="ID" type="long">
            <generator class="native">
                <param name="sequence">GB_GRADING_EVENT_S</param>
            </generator>
        </id>

        <many-to-one name="gradableObject" column="GRADABLE_OBJECT_ID" class="com.rsmart.certification.impl.hibernate.criteria.gradebook.CertGradebookObject"/>

        <property name="studentId" column="STUDENT_ID" type="string" />
    </class>
</hibernate-mapping>
